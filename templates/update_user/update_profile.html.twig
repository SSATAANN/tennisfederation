{% extends 'baseClient.html.twig' %}
    {% block body %}

<section class="my-10 py-5">
    <div class="container">
       
        {{ form_start(f, {'attr': {'class': 'multisteps-form__form', 'style': 'height: 391px;' } }) }}
        <div class="bg-white shadow rounded-lg d-block d-sm-flex">
            <div class="profile-tab-nav border-right">
                <div class="p-4">
                    <div class="img-circle text-center mb-3">
                     
                      <img src="{{ asset('uploads/user_images/' ~ app.user.getImage) }}" alt="Image"  style="width: 250px; height: 250px;">
                   
                    
                        {{ form_widget(f.imageFile, {'attr': {'class': 'form-control border dropzone dz-clickable', 'onfocus': 'focused(this)', 'onblur': 'defocused(this)'}}) }}
                    </div>
                   
                </div>
                
            </div>
          
            <div class="tab-content p-4 p-md-5" id="v-pills-tabContent">
                <div class="tab-pane fade show active" id="account" role="tabpanel" aria-labelledby="account-tab">
                    <h3 class="mb-4">Account Settings For {{app.user.name}}</h3> 
                    <div class="row mb-5">
                      
                        <div class="col-md-12 mb-5">
                            <label for="exampleFormControlInput1" class="form-label">Name</label>
                            <div class="input-group input-group-dynamic">
                              {{ form_widget(f.name, {
                                  'attr': {
                                      'class': 'form-control',
                                      'onfocus': 'focused(this)',
                                      'onfocusout': 'defocused(this)',
                                  }
                              }) }}
                            </div>
                        </div>
                        <div class="col-md-6 mb-5">
                            <label for="exampleFormControlInput1" class="form-label">Gender</label>
                            <div class="input-group input-group-dynamic">
                              {{ form_widget(f.gender, {
                                  'attr': {
                                      
                                      'class': 'form-control',
                                      'onfocus': 'focused(this)',
                                      'onfocusout': 'defocused(this)',
                                  }
                              }) }}
                            </div>
                        </div>
                        <div class="col-md-6 mb-5">
                            <label for="exampleFormControlInput1" class="form-label">Phone</label>
                            <div class="input-group input-group-dynamic">
                              {{ form_widget(f.phonenumber, {
                                  'attr': {
                                      'class': 'form-control',
                                      'onfocus': 'focused(this)',
                                      'onfocusout': 'defocused(this)',
                                  }
                              }) }}
                            </div>
                        </div>
                        
                        <div class="col-md-6 mb-5">
                            <label for="exampleFormControlInput1" class="form-label">Password</label>
                            <div class="input-group input-group-dynamic">
                              {{ form_widget(f.password.first, {
                                  'attr': {
                                      'class': 'form-control',
                                      'onfocus': 'focused(this)',
                                      'onfocusout': 'defocused(this)',
                                  }
                              }) }}
                            </div>
                        </div>
                        <div class="col-md-6 mb-5">
                            <label for="exampleFormControlInput1" class="form-label">Confirm Password</label>
                            <div class="input-group input-group-dynamic">
                              {{ form_widget(f.password.second, {
                                  'attr': {
                                      'class': 'form-control',
                                      'onfocus': 'focused(this)',
                                      'onfocusout': 'defocused(this)',
                                  }
                              }) }}
                            </div>
                        </div>
                       
                    </div>
                    {{ form_row(f.roles, { 'label': false, 'attr': {'style': 'display: none;'} }) }}
                    {{ form_row(f.email, { 'label': false, 'attr': {'style': 'display: none;'} }) }}
                    {{ form_row(f.Add, { 'label': false, 'attr': {'style': 'display: none;'} }) }}

                    <div class="text-end"> 
                        <button type="submit" class="btn btn-primary">Update</button>
                        <button type="reset" class="btn btn-light">Cancel</button>
                    </div>
                </div>
                
                </div>
            </div>
            {{ form_end(f) }}
        </div>
    </div>
</section>

</script>
