<script>
  console.log('JavaScript code executed');
  // Get the current active nav link
  const activeNavLink = document.querySelector('.nav-link.active');

  // Check if there is an active nav link
  if (activeNavLink) {
    // Get the parent collapse element
    const parentCollapse = activeNavLink.closest('.collapse');

    // Check if the parent collapse exists
    if (parentCollapse) {
      // Add the 'show' class to the parent collapse to keep it open
      parentCollapse.classList.add('show');

      // Get the parent collapse toggle link
      const parentToggleLink = document.querySelector(`[href="#${parentCollapse.id}"]`);

      // Check if the parent toggle link exists
      if (parentToggleLink) {
        // Add the 'collapsed' attribute to the parent toggle link
        parentToggleLink.setAttribute('aria-expanded', 'true');
      }
    }
  }
</script>
